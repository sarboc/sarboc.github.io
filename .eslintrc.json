{
  "extends": "eslint-config-standard",
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "jasmine"
  ],
   "rules": {
    "no-unused-vars": [0, { "vars": "local", "args": "after-used", "ignoreRestSiblings": false }],
    "react/require-extension": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "env": {
    "jasmine": true
  }
}
